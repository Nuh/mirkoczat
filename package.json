    "watch": "cross-env TZ=UTC NODE_ENV=development BABEL_ENV=development DEBUG=*,-follow-redirects,-snapdragon:*,-nodemon,-nodemon:*,-babel,-babel:* nodemon --inspect --exec npm run babel-node -- ./src/cli.js",
